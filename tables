1
yan lang muna wla akong student_subject at student_courses

query for class_schedule 
SELECT c.class_id, s.subject_code, s.subject_name, cs.day, TIME_FORMAT(cs.start_time, '%h:%i %p') AS start_time, TIME_FORMAT(cs.end_time, '%h:%i %p') AS end_time, cs.room, c.section, CONCAT(f.first_name, ' ', f.last_name) AS teacher FROM classes c JOIN subjects s ON c.subject_id = s.subject_id JOIN faculty f ON c.faculty_id = f.faculty_id JOIN class_schedule cs ON c.class_id = cs.class_id WHERE c.status = 'active' ORDER BY FIELD(cs.day, 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday'), cs.start_time;